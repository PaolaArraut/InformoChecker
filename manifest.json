{
    "manifest_version": 3,
    "name": "InformoChecker",
    "version": "1.0",
    "description": "Check research articles and paper on reliability",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "permissions": ["activeTab", "scripting"],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}